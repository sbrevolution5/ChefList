<MudSelect Label="Supply" T="Supply" @bind-Value="@supply" ToStringFunc="@converter">
    @foreach (Supply sup in supplyList)
    {

    <MudSelectItem Value="sup"></MudSelectItem>
    }
</MudSelect>

@code {
    [Parameter]
    public Supply supply { get; set; }
    [Parameter]
    public List<Supply> supplyList { get; set; }
    Func<Supply, string> converter = s => s?.Name;
}
