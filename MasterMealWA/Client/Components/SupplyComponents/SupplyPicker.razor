<MudSelect Label="Supply" T="int" @bind-Value="supply.Id">
    @foreach (var sup in supplyList)
    {

    <MudSelectItem Value="sup.Id">@sup.Name</MudSelectItem>
    }
</MudSelect>

@code {
    [Parameter]
    public Supply supply { get; set; }
    [Parameter]
    public List<Supply> supplyList { get; set; }
}
